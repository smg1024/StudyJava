class ExSort{
	int[] num;
	void init() {	// 임의 초기값
		num = new int[] {4, 3, 6, 7, 1, 4, 9};
	}
	
	// 출력
	void disp() {
		for(int n : num) {
			System.out.printf("%-3d", n);
		}
		System.out.println();
	}
	
