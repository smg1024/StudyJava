package sw.search;

public class LinkedListEx {

	public static void main(String[] args) {
		DoubleLinkedList list = new DoubleLinkedList();
		// 삽입하기
		list.insertTail("A");
		list.insertTail("B");
		list.insertTail("C");
		list.insertTail("D");
		list.insertTail("E");
		list.insertTail("F");
		
		// 삭제하기
		
		// 출력하기
		disp();
		// 역출력하기
	}

}

class DoubleLinkedList{
	Node head;
	Node tail;
	
	public DoubleLinkedList() {
		init();
	}
	
	public void disp() {
		// 출력하기
		Node trace = head.next;
		while(trace != tail) {
			System.out.print(trace.data + "\t");
			trace = trace.next;
		}
	}
	
	public void insertTail(String data) {
		Node temp = new Node();
		temp.data = data;
		
		temp.prev = tail.prev;
		tail.prev = temp;
		temp.prev.next = temp;
		temp.next = tail;
	}

	private void init() {
		head = new Node();
		tail = new Node();
		
		head.prev = head;
		head.next = tail;
		tail.prev = head;
		tail.next = tail;
	}
}

class Node{
	String data;
	Node prev;	// 앞의 노드를 가리킴
	Node next;	// 뒤의 노드를 가리킴
}