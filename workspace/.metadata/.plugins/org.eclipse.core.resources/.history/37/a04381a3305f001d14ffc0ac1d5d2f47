package sw.array;

/*
 * [문제]
 * 정수를 입력받아 팩토리얼값을 구하라.
 * 
 * [입력]
 * 5
 * 
 * [출력]
 * 120
 */
public class Ex02_Factorial {
	
	public static int result = 1;
	
	public static void fac(int n) {
		if(n==1) return;
		result *= n;
		fac(n-1);
	}

	public static void main(String[] args) {
		fac(3);
		System.out.println(result);
		
	}

}
