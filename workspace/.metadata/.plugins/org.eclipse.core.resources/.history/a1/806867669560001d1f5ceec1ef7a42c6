package sw.search;

public class BubbleSort {

	public static void main(String[] args) {
		// 1. 배열에 정렬할 자료를 넣어준다
		int[] num;
		
		// 2. bubble정렬할 곳으로 호출한다
	}

}

class Sort{
	// 1. 배열에 정렬할 자료를 넣어준다
	int[] num;
	void init() {
		num = new int[] {6, 4, 3, 7, 1, 9, 8};
	}
	
	void disp() {
		for(int i=0; i<num.length; i++) {
			System.out.printf("%2d", num[i]);
		}
	}
}