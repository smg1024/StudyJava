package sw.test;

public class Ex06_Array2 {

	public static void main(String[] args) {
		
		int[][] arr= new int[5][5];
		int cnt = 1;
		for(int r=0; r<arr.length; r++) {	// r=0,1,2,3,4
			for(int c=0; c<arr[r].length; c++) {	// c=0,1,2,3,4
				arr[r][c] = cnt++;
				System.out.printf("%3d", arr[r][c]);
			}
			System.out.println();
		}
		System.out.println("****************");
		int cnt = 1;
		for(int a=0; a<arr.length; a++) {	// r=0,1,2,3,4
			for(int b=0; b<arr[a].length; b++) {	// c=0,1,2,3,4
				arr[b][a] = cnt++;
				System.out.printf("%3d", arr[b][a]);
			}
			System.out.println();
		}
		
			
	}

}
